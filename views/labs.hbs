<h1>Labs</h1>
<div class="container">
   <div class="row">
   {{#each labs}}
      <div class="card" style="width: 18rem;">
         <img src="{{image}}" class="card-img-top" alt="...">
         <div class="card-body">
            <h5 class="card-title">{{name}}</h5>
            <p class="card-text">{{description}}<p>
                  <a href="{{link}}" class="btn btn-primary">{{name}}</a>
         </div>
      </div>
   {{/each}}
   </div>
</div>